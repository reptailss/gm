{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@codeBuilder/*": [
        "src/codeBuilder/*"
      ],
      "@crudConfig/*": [
        "src/crudConfig/*"
      ],
      "@crud/*": [
        "src/crud/*"
      ],
      "@decorators/*": [
        "src/decorators/*"
      ],
      "@export/*": [
        "src/export/*"
      ],
      "@helpers/*": [
        "src/helpers/*"
      ],
      "@imports/*": [
        "src/imports/*"
      ],
      "@modules/*": [
        "src/modules/*"
      ],
      "@render/*": [
        "src/render/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@writeModule/*": [
        "src/writeModule/*"
      ],
      "@createApp/*": [
        "src/createApp/*"
      ],
//      "os-core-ts": [
//        "src/os-core.ts"
//      ]

      //test
      "@testModules/*": [
        "src/test/testModules/*"
      ],
      "@db/*": [
        "src/test/db/*"
      ]
    }
  }
}